<div class="container">
<h1>配送先編集</h1>

<%= form_with model: @address, url: public_address_path do |f| %>

<label>郵便番号（ハイフンなし）</label>
<%= f.text_field :post_number %>

<label>住所</label>
<%= f.text_field :address %>

<label>宛名</label>
<%= f.text_field :name %>

<%= f.submit '変更内容を保存' %>
<% end %>
</div>